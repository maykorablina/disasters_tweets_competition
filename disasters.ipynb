    "# считаем все слова в датафрейме\n",
    "def most_common_words(texts):\n",
    "    word_count = {}\n",
    "    for t in texts:\n",
    "        for w in t.split():\n",
    "            if w not in word_count:\n",
    "                word_count[w] = 1\n",
    "            else:\n",
    "                word_count[w] += 1\n",
    "    word_count = dict(sorted(word_count.items(), key=lambda x: x[1], reverse = True))\n",
    "    return word_count"
   ]
  },
